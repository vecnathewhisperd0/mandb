		if $troff $troff_filter -m$macro </dev/null >/dev/null 2>&1 3>&1
		then
			man_cv_prog_troff_options="${troff_filter}-m${macro}"
			break
		fi
	done
fi

	if test -n "$man_cv_prog_troff_options"
	then
		troff="$troff $man_cv_prog_troff_options"
		{ echo "$as_me:$LINENO: result: $man_cv_prog_troff_options" >&5
echo "${ECHO_T}$man_cv_prog_troff_options" >&6; }
	else
		{ echo "$as_me:$LINENO: result: ambiguous" >&5
echo "${ECHO_T}ambiguous" >&6; }
		{ echo "$as_me:$LINENO: WARNING: please edit include/manconfig.h and add troff definition" >&5
echo "$as_me: WARNING: please edit include/manconfig.h and add troff definition" >&2;}
	fi
else
	troff="(troff not installed)"
fi

