.man1.1: replace.sed
	$(AM_V_at)$(MKDIR_P) man1
	@$(MAKE) $(AM_MAKEFLAGS) replace.sed
	$(AM_V_GEN)program=`echo '$@' | sed -e 's/\.1//' -e 's,.*/,,' -e '$(program_transform_name)'`; \
	sed -f replace.sed -e "s,%program%,$$program,g" $< > $@
.man5.5: replace.sed
